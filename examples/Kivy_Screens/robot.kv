#:kivy 1.11.1

<EyeScreen>:
    FloatLayout:
        Image:
            source: 'eyestest.png'
            size_hint: (1,1)
            pos: (0,0)
        Button:
            text: 'Go to Menu'
            on_press:
                root.manager.transition.direction = 'left'
                root.manager.current = 'menus'
            size_hint: (.1,.1)
            pos_hint:{"right":1,"top":1}


<MenuScreen>:
    BoxLayout:
        Button:
            text: 'Back to Eyes'
            on_press:
                root.manager.transition.direction = 'right'
                root.manager.current = 'eyes'
        Button:
            text: 'Quit'
